<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping package="model">
    <class name="Students" table="students">
        <id column="id" name="id">
            <generator class="native"/>
        </id>
        <property name="ln" column="ln"/>
        <property name="pw" column="pw"/>
        <property name="no"/>
        <property name="role"/>
        <property name="name" type="string"/>
        <property name="sex" type="string"/>
        <property name="cls" type="string"/>
        <property name="birthday" type="string"/>
        <property name="introduce" type="string"/>
        <property name="phone" type="string"/>
        <property name="email" type="string"/>
        <property name="grp" type="string"/>
        <property name="learnLevel" type="string" column="learn_level"/>
        <property name="power"/>
        <property name="isUsed" type="string" column="is_used"/>
        <property name="regDate" type="string" column="reg_date"/>
        <property name="passDate" type="string" column="pass_date"/>
    </class>
</hibernate-mapping>